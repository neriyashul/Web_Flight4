@{
    ViewBag.Title = "display";
}
<div>
    <img src="/photoes/export-map-share.png" alt="map" top:1 style="width:100%;height:100%" />
    <DIV id="positionDiv" style="position:absolute; top:80px; left:400px; width:20px; height:25px">
        <canvas id="myCanvas" width="200" height="100"
                style="border:1px solid black;"></canvas>
    </DIV>
</div>



<script>
    var IntervalTime = @Session["time"] * 1000
    myTimer = (function () {
        $.post( "@Url.Action("GetLocation")").done(function (xml) {
                var xmlDoc = $.parseXML( xml ),
                $xml = $( xmlDoc ),
                lon = $xml.find( "Longitude" ).text();
                lat = $xml.find( "Latitude" ).text();

            $("#positionDiv").top(lon);
            $("#prodName").left(lat);

        });
    });
    if (IntervalTime.toString == "0") {
        setInterval(myTimer, IntervalTime);
    }
    myTimer();

    ctx.beginPath();
    ctx.arc(95, 50, 40, 0, 2 * Math.PI);
    ctx.stroke();

</script>

